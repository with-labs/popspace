{
  "name": "@withso/noodle-shared",
  "version": "2.0.1",
  "description": "Shared code for Noodle micro services",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "prisma",
    "tests",
    "scripts",
    "test",
    "index.js",
    "test.js"
  ],
  "scripts": {
    "console": "node --experimental-repl-await ./scripts/console.js",
    "test:all": "NODE_ENV=test jest --runInBand",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:init": "prisma migrate deploy && node ./prisma/seed.js",
    "prisma:generate": "prisma generate",
    "build": "tsc",
    "prepare": "yarn build"
  },
  "prisma": {
    "seed": "env-cmd node prisma/seed.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/with-labs/noodle-shared.git"
  },
  "author": "Aleksei Chernikov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/with-labs/noodle-shared/issues"
  },
  "homepage": "https://github.com/with-labs/noodle-shared#readme",
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "async-lock": "^1.2.4",
    "aws-sdk": "^2.834.0",
    "btoa": "^1.2.1",
    "chalk": "^4.1.1",
    "chance": "^1.1.7",
    "commander": "^7.2.0",
    "crypto-random-string": "^3.3.0",
    "dotenv": "^8.2.0",
    "factory-girl": "^5.0.4",
    "jest": "^27.0.4",
    "jest-cli": "^27.0.4",
    "massive": "^6.6.1",
    "moment": "^2.29.1",
    "pg": "^8.5.1",
    "pg-monitor": "^1.4.1",
    "prisma": "^3.14.0",
    "redis": "^3.0.2",
    "ua-parser-js": "^0.7.28"
  },
  "devDependencies": {
    "@types/async-lock": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/massive": "^5.4.4",
    "@types/node": "^16.6.1",
    "@types/pg": "^8.6.1",
    "env-cmd": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
